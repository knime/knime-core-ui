{
  "$schema": "https://json.schemastore.org/schema-draft-v4.json",
  "description": "This schema defines the structure for a schema for each builtin validation to be used in uischema schemas.",
  "type": "object",
  "properties": {
    "type": { "const": "object" },
    "properties": {
      "type": "object",
      "properties": {
        "parameters": {
          "type": "object",
          "properties": {
            "type": { "const": "object" },
            "properties": {
              "type": "object"
            }
          }
        },
        "errorMessage": {
          "type": "object",
          "properties": {
            "type": { "const": "string" }
          }
        }
      },
      "required": ["parameters", "errorMessage"]
    }
  }
}
