{
  "$schema": "mock.def.json",
  "result": {
    "name": "DB Table Chooser",
    "data": {
      "model": {
        "tableSelection": {
          "catalogName": "testCatalog",
          "schemaName": "testSchema",
          "tableName": "testTable"
        }
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "model": {
          "type": "object",
          "properties": {
            "tableSelection": {
              "type": "object",
              "properties": {
                "catalogName": {
                  "type": "string",
                  "title": "Database name",
                  "description": "The name of the database (or catalogue) to use.",
                  "default": ""
                },
                "schemaName": {
                  "type": ["string", "null"],
                  "title": "Schema name",
                  "description": "The optional database schema name.",
                  "default": ""
                },
                "tableName": {
                  "type": "string",
                  "title": "Table name",
                  "description": "Name of the database table to write to.",
                  "default": ""
                }
              },
              "default": {
                "schemaName": "",
                "tableName": ""
              }
            }
          }
        }
      }
    },
    "ui_schema": {
      "type": "VerticalLayout",
      "elements": [
        {
          "type": "Section",
          "label": "Database Table",
          "elements": [
            {
              "type": "Control",
              "scope": "#/properties/model/properties/tableSelection",
              "options": {
                "format": "dbTableChooser",
                "catalogsSupported": true,
                "validateSchema": true,
                "validateTable": true
              }
            }
          ]
        }
      ]
    },
    "persist": {
      "type": "object",
      "properties": {
        "model": {
          "type": "object",
          "properties": {
            "tableSelection": {
              "type": "object",
              "properties": {
                "catalogName": {},
                "schemaName": {
                  "configPaths": [["schemaName_is_present"], ["schemaName"]]
                },
                "tableName": {}
              }
            }
          }
        }
      }
    },
    "flowVariableSettings": {}
  }
}
