{
  "extends": "./tsconfig.app.json",
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "lib": ["ES2023.Array"],
    "types": ["node", "jsdom"],
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./*"]
    },
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "verbatimModuleSyntax": false,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo"
  }
}
