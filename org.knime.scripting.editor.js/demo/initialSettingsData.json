{
    "name": "Checklist Dialog",
    "data": {
        "checked": true,
        "unchecked": false,
        "disabled": false
    },
    "schema": {
        "type": "object",
        "properties": {
            "checked": {
                "type": "boolean"
            },
            "unchecked": {
                "type": "boolean"
            },
            "disabled": {
                "type": "boolean"
            }
        }
    },
    "ui_schema": {
        "type": "VerticalLayout",
        "elements": [
            {
                "type": "Section",
                "label": "Checkboxes",
                "elements": [
                    {
                        "type": "Control",
                        "scope": "#/properties/checked",
                        "options": {
                            "format": "checkbox"
                        }
                    },
                    {
                        "type": "Control",
                        "scope": "#/properties/unchecked",
                        "options": {
                            "format": "checkbox"
                        }
                    },
                    {
                        "type": "Control",
                        "scope": "#/properties/disabled",
                        "options": {
                            "format": "checkbox"
                        }
                    }
                ]
            }
        ]
    },
    "flowVariableSettings": {
        "disabled": {
            "controllingFlowVariableName": "myVar"
        }
    }
}
