{
  "name": "@knime/core-ui",
  "version": "0.1.0",
  "description": "Vue-based view components for KNIME",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:NodeDialog": "pnpm _dev NodeDialog 3333",
    "dev:NodeDialog:standalone": "vite",
    "dev:TableView": "pnpm _dev TableView 4000",
    "dev:TextView": "pnpm _dev TextView 4000",
    "dev:ImageView": "pnpm _dev ImageView 4000",
    "_dev": "pnpm _createDist && run-p \"_dev:build --mode {1} --watch\" \"_dev:serve --port {2}\" --",
    "_dev:build": "vite build",
    "_dev:serve": "vite preview",
    "build": "pnpm _cleanDist && run-p type-check build:* && pnpm sbom:merge",
    "build:NodeDialog": "vite build --mode NodeDialog",
    "build:TableView": "vite build --mode TableView",
    "build:DeferredTableView": "vite build --mode DeferredTableView",
    "build:TextView": "vite build --mode TextView",
    "build:ImageView": "vite build --mode ImageView",
    "build:html": "run-p build:html:*",
    "build:html:ImageValueView": "vite build --mode html:ImageValueView",
    "build:html:CodeValueView": "vite build --mode html:CodeValueView",
    "build:html:StringCellView": "vite build --mode html:StringCellView",
    "type-check": "vue-tsc -p tsconfig.vitest.json",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "_createDist": "node scripts/createDir.js ../../../org.knime.core.ui/js-src/dist",
    "_cleanDist": "node scripts/removeDir.js ../../../org.knime.core.ui/js-src/dist",
    "lint": "run-p 'lint:* --fix'",
    "lint:js": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "ci:lint-format": "run-p ci:lint:* format:check -c",
    "ci:lint:js": "npm run lint:js && npm run lint:js -- -f json -o test-results/eslint.json",
    "ci:lint:css": "npm run lint:css -- -f json -o test-results/stylelint.json",
    "test:unit": "vitest",
    "test:integration": "vitest --mode integration",
    "coverage": "run-p coverage:*",
    "coverage:unit": "vitest run --coverage ",
    "coverage:integration": "vitest run --coverage --mode integration",
    "sbom:merge": "tsx scripts/merge-sbom.ts ../../../org.knime.core.ui/js-src/dist/sbom bom.json && node scripts/removeDir.js ../../../org.knime.core.ui/js-src/dist/sbom",
    "postinstall": "license-check -c"
  },
  "license": "UNLICENSED",
  "author": "KNIME AG, Zurich, Switzerland",
  "dependencies": {
    "@floating-ui/vue": "1.0.2",
    "@fontsource/roboto": "5.2.8",
    "@fontsource/roboto-mono": "5.2.8",
    "@jsonforms/core": "3.4.0",
    "@jsonforms/vue": "3.4.0",
    "@jsonforms/vue-vanilla": "3.4.0",
    "@knime/components": "1.43.3",
    "@knime/jsonforms": "1.16.6",
    "@knime/kds-components": "catalog:",
    "@knime/knime-ui-table": "0.6.0",
    "@knime/rich-text-editor": "1.8.0",
    "@knime/styles": "1.15.0",
    "@knime/ui-extension-service": "2.6.0",
    "@vueuse/core": "13.8.0",
    "date-fns": "2.29.3",
    "focus-trap-vue": "4.0.2",
    "highlight.js": "11.10.0",
    "lodash-es": "4.17.21",
    "modern-normalize": "3.0.1",
    "monaco-editor": "0.51.0",
    "tabbable": "6.2.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-library": "^9.2.0",
    "@cyclonedx/cyclonedx-npm": "1.16.1",
    "@knime/eslint-config": "8.3.0",
    "@knime/kds-styles": "catalog:",
    "@knime/licenses": "1.4.0",
    "@knime/ui-extension-renderer": "2.3.39",
    "@knime/utils": "1.8.0",
    "@knime/vue-headless-virtual-scroller": "0.1.3",
    "@tsconfig/node24": "24.0.3",
    "@types/color-hash": "2.0.0",
    "@types/jsdom": "21.1.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.0",
    "@types/uuid": "9.0.8",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "2.1.5",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "consola": "3.2.3",
    "eslint": "8.57.1",
    "flush-promises": "1.0.2",
    "jsdom": "20.0.3",
    "npm-run-all2": "^8.0.4",
    "postcss-mixins": "9.0.4",
    "postcss-preset-env": "^10.4.0",
    "prettier": "3.0.0",
    "rollup-plugin-sbom": "^3.0.0",
    "stylelint": "16.25.0",
    "tsx": "^4.20.6",
    "type-fest": "4.3.1",
    "typescript": "5.4.5",
    "vite": "5.4.11",
    "vite-svg-loader": "5.1.0",
    "vitest": "2.1.5",
    "vue": "catalog:",
    "vue-tsc": "2.1.10"
  },
  "engines": {
    "node": "24.x"
  }
}
